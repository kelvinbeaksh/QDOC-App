{
  "name": "qdoc-portal",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/compatible": "1.0.8",
    "@ant-design/icons": "4.5.0",
    "@beam-australia/react-env": "^3.0.8",
    "@twilio-labs/plugin-rtc": "^0.8.2",
    "@twilio/conversations": "^1.2.3",
    "@twilio/video-processors": "^1.0.1",
    "@twilio/video-room-monitor": "^1.0.0-beta.1",
    "@types/node": "14.14.32",
    "@types/react-dom": "17.0.2",
    "@types/react-router-dom": "5.1.7",
    "@types/validate.js": "^0.11.0",
    "antd": "4.13.1",
    "antd-mobile": "^5.0.0-beta.21",
    "antd-mobile-icons": "^0.2.2",
    "array-move": "^4.0.0",
    "axios": "^0.24.0",
    "copyfiles": "^2.4.1",
    "d3-timer": "^3.0.1",
    "dotenv": "^8.2.0",
    "firebase": "^8.4.1",
    "google-map-react": "2.1.9",
    "jsrsasign": "^10.3.0",
    "linkify-it": "^3.0.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "^6.1.0",
    "react-pose": "4.0.10",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "react-sortable-hoc": "^2.0.0",
    "react-spring": "^9.3.0",
    "sass": "^1.86.3",
    "twilio": "^3.68.0",
    "twilio-video": "^2.18.0",
    "validate.js": "^0.13.1",
    "yup": "0.32.9"
  },
  "scripts": {
    "postinstall": "rimraf public/virtualbackground && copyfiles -f node_modules/@twilio/video-processors/dist/build/* public/virtualbackground",
    "prestart": "mkdir -p public/env && react-env --dest ./public/env",
    "start": "react-app-rewired start --jsx react",
    "build": "GENERATE_SOURCEMAP=false react-app-rewired --max-old-space-size=2048 build",
    "test": "react-app-rewired test --watchAll=false",
    "test:clearCache": "react-app-rewired test --clearCache",
    "test:showConfig": "react-app-rewired test --showConfig",
    "test:watch": "react-app-rewired test --watchAll",
    "eject": "react-scripts eject",
    "lint": "tsc --noEmit && eslint --config .eslintrc.js --ignore-path .eslintignore --ext .js,.jsx,.ts,.tsx 'src/' '__tests__/'",
    "lint:fix": "npm run lint -- --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "htmlWhitespaceSensitivity": "ignore",
    "singleQuote": false,
    "jsxSingleQuote": true,
    "jsxBracketSameLine": false,
    "printWidth": 100,
    "trailingComma": "none",
    "function-paren-newline": "off"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^5.1.2",
    "@testing-library/user-event": "^13.1.5",
    "@types/d3-timer": "^3.0.0",
    "@types/history": "4.7.8",
    "@types/jest": "^26.0.22",
    "@types/jest-when": "^2.7.2",
    "@types/react": "^17.0.3",
    "@types/yup": "0.29.11",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-import": "1.13.3",
    "customize-cra": "1.0.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^5.0.0",
    "http-status-codes": "^2.1.4",
    "husky": "^6.0.0",
    "jest": "^26.6.0",
    "jest-html-reporter": "^3.3.0",
    "jest-svg-transformer": "^1.0.0",
    "jest-when": "^3.4.2",
    "prettier": "2.2.1",
    "react-app-rewire-less-modules": "1.3.0",
    "react-app-rewired": "2.1.8",
    "sequelize-cli": "^6.2.0",
    "ts-jest": "^26.5.4",
    "typescript": "^3.9.7"
  }
}
