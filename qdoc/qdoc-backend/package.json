{
  "name": "qdoc",
  "version": "1.0.0",
  "description": "qdoc",
  "scripts": {
    "start": "ts-node-dev --respawn --transpile-only src/server.ts",
    "db:setup": "sequelize db:drop; sequelize db:create && sequelize db:migrate",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:delete-seed": "sequelize db:seed:undo:all",
    "prod": "rm -rf ./dist/* && tsc -p ./tsconfig.prod.json && node ./dist/server.js",
    "test": "npm run lint && TZ='Asia/Singapore' ./node_modules/.bin/jest --detectOpenHandles --coverage --forceExit ",
    "lint": "tsc --noEmit && eslint --ext .js,.jsx,.ts,.tsx 'src/' '__tests__/'",
    "lint:fix": "npm run lint -- --fix",
    "test:watch": "TZ='Asia/Singapore' jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qdoctech/qdoc.git"
  },
  "engines": {
    "node": "^20.11.1",
    "npm": "^10.2.4"
  },
  "dependencies": {
    "express-validator": "^7.0.1",
    "@types/express-validator": "^3.0.0",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-winston": "^4.2.0",
    "firebase-admin": "^12.0.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "sequelize": "^6.37.1",
    "sequelize-typescript": "^2.1.6",
    "twilio": "^4.22.0",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-transport": "^4.7.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.19",
    "@types/rosie": "^0.0.45",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@types/validator": "^13.11.9",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^6.0.0",
    "jest": "^29.7.0",
    "jest-extended": "^0.11.5",
    "jest-html-reporter": "^3.3.0",
    "jest-when": "^3.6.0",
    "prettier": "^3.2.5",
    "rosie": "^2.1.1",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0"
  }
}
